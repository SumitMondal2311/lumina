{
    "$schema": "https://www.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "noImplicitAny": false,
        "strictBindCallApply": false,
        "noFallthroughCasesInSwitch": false
    }
}
